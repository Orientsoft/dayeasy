    <compilation debug="true" targetFramework="4.5.1" />
    <httpRuntime targetFramework="4.5" />
    <customErrors mode="On" defaultRedirect="http://www.daydasy.dev/error/500">
      <error statusCode="404" redirect="http://www.daydasy.dev/error/404" />
      <error statusCode="500" redirect="http://www.dayeasy.dev/error/500" />
    </customErrors>
  <membership defaultProvider="MySQLMembershipProvider">
