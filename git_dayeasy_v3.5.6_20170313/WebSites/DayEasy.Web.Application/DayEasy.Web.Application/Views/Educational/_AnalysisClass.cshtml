@model DayEasy.Contracts.Dtos.Examination.ExamRanksDto
@{
    ViewBag.Title = "班级分析 - " + Model.Name;
}
<div class="dy-ranks">
    <div class="dy-actions dy-key-actions">
        <h3>重点率分析</h3>
        <div class="dy-key-rate" id="classKeys">
            <span>重点率：</span>
            <select class="dy-key-type">
                <option value="0">分数</option>
                <option value="1" selected="selected">比例</option>
            </select>
            <div class="input-group add-right">
                <input type="text" value="75" class="w4" />
                <label>%</label>
            </div>
            <label>A卷平均分合格分数：</label>
            <div class="input-group add-left">
                <label>≥</label>
                <input type="text" value="60" class="w4" />
            </div>
            <label>A卷平均分不合格分数：</label>
            <div class="input-group add-left">
                <label>&lt;</label>
                <input type="text" value="60" class="w4" />
            </div>
            <button class="dy-btn dy-btn-info">
                <i class="iconfont dy-icon-jisuan" style="margin-right: 4px"></i>
                计算
            </button>
        </div>
    </div>
    <div class="dy-table-wrap">
        <div class="dy-table-head">
            <table class="dy-table" style="width: 1556px">
                <colgroup>
                    <col style="width: 156px" />
                    <col style="width: 120px" />
                    <col style="width: 95px" />
                    <col style="width: 95px" />
                    <col style="width: 140px" />
                    <col style="width: 140px" />
                    <col style="width: 270px" />
                    <col style="width: 270px" />
                    <col style="width: 270px" />
                </colgroup>
                <thead>
                    <tr>
                        <th>班级</th>
                        <th>圈主</th>
                        <th>应查人数</th>
                        <th>实际人数</th>
                        <th class="th-group">
                            <div class="d-table-group">
                                <span>总平均分</span>
                                <span>差值</span>
                            </div>
                        </th>
                        <th class="th-group">
                            <div class="d-table-group">
                                <span>均值比</span>
                                <span>差值</span>
                            </div>
                        </th>
                        <th class="th-group">
                            <div class="d-table-group">
                                <span style="width:118px">重点率上线人数</span>
                                <span style="width:70px">比例</span>
                                <span style="width:70px">差值</span>
                            </div>
                        </th>
                        <th class="th-group">
                            <div class="d-table-group col3">
                                <span style="width:118px">A卷合格人数</span>
                                <span style="width:70px">比例</span>
                                <span style="width:70px">差值</span>
                            </div>
                        </th>
                        <th class="th-group">
                            <div class="d-table-group col3">
                                <span style="width:118px">A卷不合格人数</span>
                                <span style="width:70px">比例</span>
                                <span style="width:70px">差值</span>
                            </div>
                        </th>
                    </tr>
                </thead>
            </table>
        </div>
        <div class="dy-table-body">
            <table class="dy-table" style="width: 1556px">
                <colgroup>
                    <col style="width: 156px" />
                    <col style="width: 120px" />
                    <col style="width: 95px" />
                    <col style="width: 95px" />
                    <col style="width: 70px" />
                    <col style="width: 70px" />
                    <col style="width: 70px" />
                    <col style="width: 70px" />
                    <col style="width: 130px" />
                    <col style="width: 70px" />
                    <col style="width: 70px" />
                    <col style="width: 130px" />
                    <col style="width: 70px" />
                    <col style="width: 70px" />
                    <col style="width: 130px" />
                    <col style="width: 70px" />
                    <col style="width: 70px" />
                </colgroup>
                <tbody id="classKeysBody">
                    <tr>
                        <td colspan="11">
                            <div class="dy-loading">
                                <i></i>
                            </div>
                        </td>
                    </tr>
                </tbody>
                <tfoot id="classKeysFoot"></tfoot>
            </table>
        </div>
    </div>
</div>
<div class="dy-ranks dy-ranks-split">
    <div class="dy-actions dy-key-actions">
        <h3>学生分层分析</h3>
        <div class="dy-key-rate" id="classLayers">
            <label>A层：</label>
            <div class="input-group add-right">
                <input type="text" value="10" class="w4" min="0" max="100" />
                <label>%</label>
            </div>
            <label>B层：</label>
            <div class="input-group add-right">
                <input type="text" value="30" class="w4" min="0" max="100" />
                <label>%</label>
            </div>
            <label>C层：</label>
            <div class="input-group add-right">
                <input type="text" value="30" class="w4" min="0" max="100" />
                <label>%</label>
            </div>
            <label>D层：</label>
            <div class="input-group add-right">
                <input type="text" value="20" class="w4" min="0" max="100" />
                <label>%</label>
            </div>
            <label>E层：</label>
            <div class="input-group add-right">
                <input type="text" value="10" class="w4" min="0" max="100" />
                <label>%</label>
            </div>
            <button class="dy-btn dy-btn-info">
                <i class="iconfont dy-icon-jisuan" style="margin-right: 4px"></i>
                计算
            </button>
        </div>
    </div>
    <div class="dy-table-wrap">
        <div class="dy-table-head">
            <table class="dy-table">
                <colgroup>
                    <col style="width: 156px" />
                    <col style="width: 120px" />
                    <col style="width: 95px" />
                    <col style="width: 95px" />
                    <col style="width: 140px" />
                    <col style="width: 140px" />
                    <col style="width: 130px" />
                    <col style="width: 130px" />
                    <col style="width: 130px" />
                </colgroup>
                <thead>
                    <tr>
                        <th>班级</th>
                        <th>圈主</th>
                        <th>应查人数</th>
                        <th>实际人数</th>
                        <th>A层人数<em></em></th>
                        <th>B层人数<em></em></th>
                        <th>C层人数<em></em></th>
                        <th>D层人数<em></em></th>
                        <th>E层人数<em></em></th>
                    </tr>
                </thead>
            </table>
        </div>
        <div class="dy-table-body">
            <table class="dy-table">
                <colgroup>
                    <col style="width: 156px" />
                    <col style="width: 120px" />
                    <col style="width: 95px" />
                    <col style="width: 95px" />
                    <col style="width: 140px" />
                    <col style="width: 140px" />
                    <col style="width: 130px" />
                    <col style="width: 130px" />
                    <col style="width: 130px" />
                </colgroup>
                <tbody id="classLayersBody">
                    <tr>
                        <td colspan="9">
                            <div class="dy-loading"><i></i></div>
                        </td>
                    </tr>
                </tbody>
            </table>
        </div>
        <div class="dy-table-foot">
            <table class="dy-table">
                <colgroup>
                    <col style="width: 156px" />
                    <col style="width: 120px" />
                    <col style="width: 95px" />
                    <col style="width: 95px" />
                    <col style="width: 140px" />
                    <col style="width: 140px" />
                    <col style="width: 130px" />
                    <col style="width: 130px" />
                    <col style="width: 130px" />
                </colgroup>
                <tfoot id="classLayersFoot"></tfoot>
            </table>
        </div>
    </div>
</div>
